    "angular-filter": "^0.5.15",
    "angular-material": "^1.1.1",
    "angular-material-expansion-panel": "^0.7.0",
    "angular-material-simple-components": "^1.1.0",
    "angular-messages": "^1.6.1",
    "angular-resource": "^1.6.0",
    "angular-ui-router": "^0.4.2",
    "chart.js": "^2.5.0",
    "moment": "^2.15.1",
    "ngMask": "https://github.com/candreoliveira/ngMask.git"
